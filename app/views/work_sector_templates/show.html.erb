<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Work sector #<%= @work_sector.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/work_sectors" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <div class="col">
        <a href="/work_sectors/<%= @work_sector.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit work sector
        </a>
      </div>

      <div class="col">
        <a href="/delete_work_sector/<%= @work_sector.id %>" class="btn btn-block btn-outline-secondary">
          Delete work sector
        </a>
      </div>
    </div>

    <dl>
      <dt>
        Sector name
      </dt>
      <dd>
        <%= @work_sector.sector_name %>
      </dd>

      <dt>
        Created at
      </dt>
      <dd>
        <%= time_ago_in_words(@work_sector.created_at) %> ago
      </dd>

      <dt>
        Updated at
      </dt>
      <dd>
        <%= time_ago_in_words(@work_sector.updated_at) %> ago
      </dd>
    </dl>
  </div>
</div>

<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <%= render partial: "work_sector_templates/work_sector_people" %>
  </div>
</div>
